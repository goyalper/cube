{
  "name": "cube-solver-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i src/style.css -o app/static/css/style.css --minify",
    "build:js": "esbuild src/app.ts --bundle --outfile=app/static/js/app.js --minify",
    "build": "npm run build:css && npm run build:js"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.0",
    "esbuild": "^0.17.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.0.0"
  }
}
